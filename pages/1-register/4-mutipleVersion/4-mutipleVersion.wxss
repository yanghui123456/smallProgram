/* pages/1-register/4-mutipleVersion/4-mutipleVersion.wxss */
page,.version{
  width:100%;
  height:100%;
  position: relative;
  background: rgb(245,245,245);
}
.version{
  overflow: auto;
  -webkit-overflow-scrolling:touch;
}
/* 滑块swiper */
.swiper{
  width:100%;
  padding-top:30px;
  background-image: url('http://oss.baoxian.xujinkeji.com/broker/openTeamBack.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.title1{
  margin-bottom:3px;
}
.title2{
  font-size:29px;
}
.partFour{
  width:100%;
  padding:25px 14% 20px 14%;
  box-sizing: border-box;
  overflow: hidden;
}
.fourItem{
  width:25%;
  float: left;
}
.fourImg{
  width:34px;
  height:34px;
}
.swiperGpup{
  width:100%;
  height:400px;
}
.swaiperImg{
  width:95%;
  height:360px
}
.swaiperbtn{
  width:80%;
  height:45px;
  position: absolute;
  background: transparent;
  bottom:69px;
  left:10%;
}
/* 全生命周期客户维护 */
.cycleTitle{
  color:rgb(253,137,2);
  padding:20px 0;
}
.imgBox{
  width:100%;
  height:52%;
  padding-bottom:28px;
}
.cycleimg{
  width:85.4%;
  height:100%;
  margin:0 0 0 7.3%;
}
/* 分三部分 */
.cycleDetail{
  width:100%;
  border-top:1px dashed rgb(245,245,245);
  padding-top:28px;
}
.threePart{
  width:100%;
  padding:24px 3.6%;
  overflow: hidden;
  box-sizing: border-box;
}
.partItem{
  width:33.3%;
  float: left;
}
.partImg{
  width:40px;
  height:40px;
}
.partLeft{
  margin-left:8%;
}
.partRight{
  margin-right:8%;
}