page,.weihu,.list{
  width:100%;
  height:100%;
  background: rgb(245,245,245);
}
/*无数据  */
.nothing{
  width:69px;
  height:70px;
  margin-top:50%;
}
.no-text{
  color:rgb(204,204,204);
  margin-bottom:20px;
}
.invit{
  border-radius: 5px;
  padding: 10px 18px;
}
/*有数据  */
.weihu,.list{
  position: relative;
  overflow: auto;
  -webkit-overflow-scrolling:touch;
}
.weihuTab{
  width:100%;
  overflow: hidden;
  padding:15px 22%;
  box-sizing: border-box; 
  border:1px solid rgb(245,245,245);
}
.weihuItem1,.weihuItem2,.activeTab1,.activeTab2{
  width:50%;
  height:27px;
  line-height: 27px;
  border:1px solid rgb(246,161,34);
  box-sizing: border-box;
  float: left;
  font-size:12px;
  color:rgb(246,161,34);
  overflow: hidden;
}
.weihuItem1{
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.weihuItem2{
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
}
.activeTab1{
  background:rgb(246,161,34);
  color:white;
  overflow: hidden;
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.activeTab2{
  background:rgb(246,161,34);
  color:white;
  overflow: hidden;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
}
.item,.info,.detail{
  width:100%;
}
.item{
  height:95px;
  border-bottom:1px solid rgb(245,245,245);
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
}
.cover{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
}
.info,.detail{
  padding-left:22%;
  text-align: left;
  color:rgb(103,103,103);
  box-sizing: border-box;
}
.info{
  margin:24px 0 5px 0;
  height:25px;
  line-height: 25px;
}
.down{
  color:rgb(0,155,255);
  position: absolute;
  bottom:24px;
  left:0;
  padding-left:22%;
  text-align: left;
  box-sizing: border-box;
}
.info-name{
  display: inline-block;
  max-width:40%;
  height:25px;
  line-height: 23px;
  font-size:19px;
  margin-right:15px;
  vertical-align: middle;
}
.headImg{
  width:50px;
  height:50px;
  border-radius: 50%;
  position: absolute;
  top:22.5px;
  left:4%;
}
.call{
  width:34px;
  height:34px;
  position: absolute;
  top:30.5px;
  right:4%;
}
.tgInvit{
  position: absolute;
  top:30.5px;
  right:4%;
  border-radius: 3px;
  padding:3px 6px;
}
.no-people{
  display: inline-block;
  color:rgb(0,155,255);
  padding:5px 0;
  margin:10px 0 13px 0;
}
/*发送文本信息  */
.text-mask,.mask,.box-head-img,.box-text{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  overflow: hidden;
}
.mask{
  background: black;
  opacity: 0.5;
}
.box{
  width:88%;
  margin-left:6%;
  border-radius: 10px;
  overflow: hidden;
  position: absolute;
  top:50%;
  margin-top:-124px;
}
.box-bottom,.box-head,.box-conten{
  width:100%;
}
.box-head{
  height:72px;
  line-height: 72px;
  font-size:19px;
  position: relative;
}
.box-head-colse{
  width:14px;
  height:14px;
  position: absolute;
  top:20px;
  right:18px;
  padding:10px;
  z-index:99;
}
.box-conten{
  padding:30px 20px;
  box-sizing: border-box;
  text-align: left;
}
.box-bottom{
  background: rgb(245,248,248);
  height:46px;
  line-height: 46px;
}
.bottom-btn{
  width:50%;
  height:100%;
  float: left;
  border-top:1px solid rgb(238,238,238);
  box-sizing: border-box;
  position: relative;
}
.copyText{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  color:transparent;
}