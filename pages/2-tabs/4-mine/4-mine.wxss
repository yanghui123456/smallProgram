page,.mine{
  width:100%;
  height:100%;
  background: rgb(245,245,245);
}
page{
  position: relative;
}
.mine{
  position: absolute;
  top:0;
  left:0;
}
.minecroll{
  position: absolute;
  top:0;
  left:0;
  height:100%; 
  width:100%;
  background: rgb(245,245,245);
}
.wxNum{
  width:100%;
  padding-right:4%;
  box-sizing: border-box;
  text-align: right;
  position: absolute;
  top:20px;
  left:0;
}
.wx-icon{
  width:12px;
  height:10px;
  margin-right:4px;
}
.advert{
  width:100%;
  height:70px;
}
.advert-img{
  width:92%;
  height:50px;
  margin:10px 4%;
  border-radius: 5px;
}
.recharge{
  display: inline-block;
  height:20px;
  line-height: 20px;
  padding:0 10px;
  color:rgb(101,214,152);
  border:1px solid rgb(101,214,152);
  border-radius: 3px;
  position: absolute;
  top:15px;
  right:12%;
  z-index:99;
}
.htb{
  display: inline-block;
  height:51px;
  line-height: 51px;
  color:rgb(153,153,153);
  position: absolute;
  top:0;
  right:29%;
}
.number{
  color:red;
}
.mine-list{
  width:100%;
  padding:0 4%;
  height:51px;
  line-height: 50px;
  border-bottom:1px solid rgb(245,245,245);
  box-sizing: border-box;
  position: relative;
}
.mineAll{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  z-index:100;
}
.list-icon{
  width:16px;
  height:16px;
  vertical-align: middle;
  margin:-2px 10px 0 0;
}
.list-join{
  width:24px;
  height:24px;
  position: absolute;
  right:4%;
  top:13px;
}
.rightEl{
  position: absolute;
  top:0;
  right:12%;
  font-size:14px;
}
/*开通会员成  */
.success,.mask{
  width:100%;
  height:100%;
  position: fixed;
  top:0;
  left:0;
}
.mask{
  background: black;
  opacity: 0.5; 
}
.mask-content{
  width:88%;
  height:285px;
  position: absolute;  
  top:50%; 
  left:6%; 
  margin-top:-142.5px; 
  border-radius: 10px;
  z-index:200;
}
.mask-head-img{
  width:100%;
  height:140px;
  margin-top:-80px;
}
.close-img{
  width:25px;
  height:25px;
  padding:5px;
  position: absolute;
  top:13px;
  right:10px;
}
.submit, .submit-tips{
  width:100%;
}
.submit{
  font-size:17px;
  margin-top:30px;
}
.submit-tips{
  margin:8px 0 40px 0;
}
.complate{
  width:85%;
  height:45px;
  line-height: 45px;
  background: rgb(246,161,34);
  margin-left: 7.5%;
  border-radius: 5px;
}