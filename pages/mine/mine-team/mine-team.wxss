/* pages/mine/mine-team/mine-team.wxss */
page,.team{
  width:100%;
  height:100%;
  position: relative;
}
.teamTop{
  width:100%;
  height:100%;
  padding-bottom:45px;
  box-sizing: border-box;
  overflow: auto;
  -webkit-overflow-scrolling:touch;
}
.teamBack{
  width:100%;
  height:110px;
  padding:15px 3.4% 0 3.4%;
  position: relative;
  box-sizing: border-box;
  background: url('http://oss.baoxian.xujinkeji.com/broker/myTeam.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.managerSetting{
  position: absolute;
  top:15px;
  right:3.4%;
  background: pink;
  width:20px;
  height:20px;
}
.teamSet{
  width:100%;
  height:45px;
  line-height:45px;
}
.identity{
  position: absolute;
  top:17px;
  right:4%;
}
.authorMember{
  width:100%;
  padding:0 4%;
  height:57px;
  line-height: 57px;
  box-sizing: border-box;
  position: relative;
}
.memberImgList{
  position: absolute;
  top:-17px;
  left:4%;
}
.imgItem{
  width:34px;
  height:34px;
  border:1px solid white;
  border-radius: 50%;
  box-sizing: border-box;
  z-index:100;
  position: absolute;
}
.aa{
  z-index: 99;
  margin-left:23px;
}
.bb{
  z-index: 98;
  margin-left:46px;
}
.cc{
  z-index: 97;
  margin-left:69px;
}
.dd{
  z-index: 96;
  margin-left:92px;
}
.invitMember,.buyAuthor{
  height:26px;
  line-height: 26px;
  background: rgb(1,150,253);
  padding:0 11px;
  border-radius: 3px;
  border:1px solid rgb(0,133,226);
  box-sizing: border-box;
  float: right;
  margin-top:-13px;
  box-shadow: 0px 2px 2px 1px #C1D0D7;
}
.invitIcon{
  width:10px;
  height:10px;
  /* vertical-align: middle; */
  margin-right:5px;
}
.buyAuthor{
  height:26px;
  line-height: 26px;
  padding:0 6px;
  border-color:rgb(234,234,234);
  box-sizing: border-box;
  background: white;
  float: right;
  margin-top:-13px;
  margin-left:4px;
  box-shadow: 0px 2px 2px 1px #ddd;
}
.rank{
  width:56px;
  height:56px;
  position: absolute;
  top:-50%;
  right:6.6%;
  border-radius: 20px;
    box-shadow: 0px 2px 2px 1px #eee;
}
.rankNum{
  display: inline-block;
  color:rgb(253,114,49);
  height:28px;
  line-height: 40px;
  width:100%;
  position: absolute;
  top:0;
  left:0;
  font-size:22px;
}
.rankText{
  width:100%;
  height:28px;
  line-height: 26px;
  position: absolute;
  bottom:0;
  left:0;
}
.crown{
  width:28px;
  height:28px;
  position: absolute;
  top:-14px;
  right:-8px;
}
/* 成员列表 */
.familyMember{
  width:100%;
  padding:0 4%;
  box-sizing: border-box;
}
.title{
  font-size:20px;
  padding-top:28px;
  position: relative;
}
.touch{
  font-weight: normal;
  position: absolute;
  bottom:5px;
  right:0;
}
.sort{
  width:10px;
  height:10px;
  vertical-align: middle;
}
.teamList{
  width:100%;
  border:1px solid rgb(255,221,123);
  box-sizing: border-box;
  border-radius: 5px;
  margin:10px 0 10px 0;
  overflow: hidden;
  position: relative;
}
.teamListAll{
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
  z-index: 999;
}
.teamListNo{
  border-color:rgb(221,221,221);
}
.listTop{
  width:100%;
  height:53px;
  line-height: 53px;
  background: rgb(255,240,201);
  padding-left:60px;
  box-sizing: border-box;
  position: relative;
}
.listTopNo{
  background: rgb(247,247,247);
} 
.listImg{
  width:34px;
  height:34px;
  position: absolute;
  top:9.5px;
  left:15px;
  border:1px solid white;
  box-sizing: border-box;
  border-radius: 50%;
}
.listBottom{
  width:100%;
  height:70px;
  background: rgb(255,248,219);
}
.listBottomNo{
  background: white;
}
.mingci{
  height:100%;
  line-height: 53px;
  position: absolute;
  top:0;
  right:25px;
  color:rgb(152,154,153);
}
.adopt,.refuse{
  float:left;
  height:25px;
  line-height:23px;
  margin-top:14px;
  padding:0 6px;
  border-radius: 5px;
  border:1px solid rgb(211,170,116);
  box-sizing: border-box;
}
.adopt{
  margin-left:6px;
}
.mingciBig{
  color:black;
  font-weight: 700;
  font-size:15px;
}
.mingciCall{
  width:20px;
  height:20px;
  position: absolute;
  top:16.5px;
  right:25px;
  z-index:2000;
}
.join{
  color:rgb(248,161,30);
  z-index:2000;
}
.oneThird{
  color:rgb(252,193,33);
}
.oneThirdNo{
  color:rgb(153,153,153);
}
.hasColorFont{
  color:rgb(255,113,49);
  vertical-align: middle;
}
/* 底部 */
.teamBottom{
  width:100%;
  height:45px;
  line-height: 45px;
  position: fixed;
  bottom:0;
  left:0;
  border-top:1px solid rgb(241,241,241);
  box-sizing: border-box;
  z-index:999;
}
.bottomIcon{
  width:15px;
  height:15px;
}
/* 邀请成员弹窗 */
.addToast,.mask{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  z-index:3000;
}
.mask{
  background: black;
  opacity: 0.8;
}
.addContent{
  width:92%;
  background: white;
  z-index:3001;
  position: absolute;
  left:4%;
  top:40px;
  border-radius: 10px;
  overflow: hidden;
}
.manageSetContent{
  width:100%;
  background: white;
  z-index:2000;
  position: fixed;
  left:0;
  bottom:0;
  overflow: hidden;
}
.addHead{
  width:100%;
  height:100px;
  position: relative;
  background-image: url('http://oss.baoxian.xujinkeji.com/broker/wh-mask-head.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.addClose{
  width:20px;
  height:20px;
  position: absolute;
  top:10px;
  right:5.4%;
}
.brokerImg{
  width:80px;
  height:80px;
  border:1px solid white;
  position: absolute;
  bottom:-25px;
  left:50%;
  margin-left:-40px;
  box-sizing: border-box;
  border-radius: 50%;
  box-shadow: 0px 3px 2px 1px #eee;
}
.addName{
  font-size:17;
  padding:37px 0 36px 0;
}
.hello{
  width:100%;
  margin-bottom:25px;
  padding-left:5.4%;
  box-sizing: border-box;
}
.addInvit{
  width:89.2%;
  height:45px;
  line-height: 45px;
  border-radius: 5px;
  margin:0 0 10px 5.4%;
}