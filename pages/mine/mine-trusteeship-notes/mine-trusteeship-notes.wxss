page,.tru-notes{
  width:100%;
  height:100%;
  background: rgb(245,245,245);
}
.tru-group{
  width:100%;
  margin-top:20px;
}
.time{
  background: rgb(187,187,187);
  padding:5px;
  border-radius: 5px;
}
.text{
  margin-top:15px;
  width:100%;
  padding:62px 4%;
  box-sizing: border-box;
  text-align: left;
  position: relative;
}
.start{
  width:13px;
  height:11px;
  position: absolute;
  top:40px;
  left:4%;
}
.end{
  width:13px;
  height:11px;
  position: absolute;
  bottom:40px;
  right:4%;
}
.nothing{
  width:69px;
  height:70px;
  margin-top:50%;
}
.no-text{
   color:rgb(204,204,204);
}