page,.teamIn{
  width:100%;
  height:100%;
  position: relative;
  background: rgb(245,245,245);
  overflow: auto;
  -webkit-overflow-scrolling:touch;
}
.inHead{
  width:97.4%;
  background: rgb(218, 209, 209);
  padding-bottom:135px;
  margin:10px 0 20px 1.33%;
  background-image: url('http://oss.baoxian.xujinkeji.com/broker/invitHeadBack.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.inImg{
  width:46px;
  height:46px;
  border-radius: 50%;
  margin:30px 0 3px 0;
}
.inName{
  width:100%;
  font-size:17px;
  color:rgb(277,22,52);
  margin-bottom:25px;
}
.inContent{
  padding:0 6.4%;
  text-align: left;
  color:rgb(63,63,61);
  box-sizing: border-box;
}
.acceptIn{
  width:92%;
  height:45px;
  line-height: 45px;
  margin-left:4%;
  border-radius: 5px;
  margin-bottom:28px;
}
.hasIn,.hasInList{
  width:100%;
}
.hasInList{
  padding-left:10.6%;
  padding-right:10.6%;
  box-sizing: border-box;
  overflow: hidden;
}
.hasInItem{
  width:32px;
  height:32px;
  float: left;
  box-sizing: border-box;
  margin:0 4px 4px 0;
}