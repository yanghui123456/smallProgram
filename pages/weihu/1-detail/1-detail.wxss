page,.wh-detail,.content{
  width:100%;
  height:100%;
  background: rgb(245,245,245);
}
.wh-detail{
  position: relative;
}
.content{
  position: absolute;
  top:0;
  left:0;
  padding-bottom:50px;
  box-sizing: border-box;
  overflow: auto;
  -webkit-overflow-scrolling:touch;
}
.head,.name,.family-member,.member-add,.programme,.insurance-list,.menber-list,.policy-list{
  width:100%;
}
.head{
  height:103px;
  border-bottom:1px solid rgb(245,245,245);
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
  background: white;
}
.name,.wh-record{
  padding-left:20%;
  box-sizing: border-box;
  text-align: left;
}
.name{
  height:30px;
  line-height: 30px;
  font-size:18px;
  margin:22px 0 6px 0;
}
.wh-record{
  height:20px;
  line-height: 20px;
  color:rgb(0,150,255);
  position: absolute;
  bottom:28px;
  left:0;
}
.head-img{
  width:50px;
  height:50px;
  border-radius: 50%;
  position: absolute;
  top:24px;
  left:4%;
}
.head-hb,.head-wx,.head-call{
  width:34px;
  height:34px;
  border-radius: 50%;
  position: absolute;
  top:32.5%;
}
.head-wx{
  right:27.2%;
}
.head-hb{
  right:15.4%; 
}
.head-call{
  right:4%; 
}
.family-member,.insurance-list{
  background: white;
  margin-bottom:10px;
}
.member-head{
  height:40px;
  line-height: 40px;
  color:rgb(67,66,80);
  padding-left:4%;
  box-sizing: border-box;
  position: relative;
}
.line{
  width:3px;
  height:15px;
  background: rgb(254,156,93);
  position: absolute;
  top:12.5px;
  left:0;
}
.add{
  padding-left:10px;
  color:rgb(0,150,255);
  height:100%;
  position: absolute;
  top:0;
  right:4%;
}
.member-add{
  margin-bottom:10px;
  padding:18px 0 45px 0;
}
.add-btn{
   color:rgb(0,150,255);
   border:1px solid rgb(0,150,255);
   border-radius: 5px;
   padding:9px 30px; 
}
/*有家庭成员  */
.menber-list{
  padding:20px 6.4%;
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
}
.member-item{
  width:50%;
  height:63px;
  border-right:1px dashed rgb(228,228,228);
  border-bottom:1px dashed rgb(228,228,228);
  float: left;
  box-sizing: border-box;
  position: relative;
}
.allSpan{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  z-index:100;
}
.member-img{
  width:43px;
  height:43px;
  position: absolute;
  top:10px;
  left:0;
}
.member-name{
  margin-top:13px;
  position: relative;
}
.member-name,.member-sex{
  width:100%;
  padding-left:40px;
  box-sizing: border-box;
  overflow: hidden;
}
.name-width{
  max-width:45%;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.relation{
  background: rgb(255,188,118);
  padding:0px 6px;
  border-radius: 8px;
  margin-left:5px;
  position:absolute;
  top:4px;
  left:57%;
}
.member-sex{
  color:rgb(102,102,102);
  margin-top:2px;
}
.bottomLine{
  width:100%;
  height:2px;
  background: white;
  position: absolute;
  bottom:20px;
  left:0;
  z-index:999;
}
.rightLine{
  width:2px;
  height:100%;
  position: absolute;
  top:0;
  right:6.2%;
  z-index:999;
}
/*保单列表  */

/*生成保障方案  */
.programme{
  height:50px;
  line-height: 50px;
  border-top:1px solid rgb(190,189,187);
  box-shadow: 0 -3px 5px 0px #E0DCDB;
  position: fixed;
  bottom:0;
  left:0;
}
/*添加家庭成员  */
.member-list,.mask{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
}
.mask{
  background: black;
  opacity: 0.5;
}
.real-name{
  width: 100%;
  position:fixed;
  bottom:0;
  left:0;
  z-index:200;
}
.real-head,.real-bottom{
  width: 100%;
  height: 55px;
  line-height: 55px;
  position:relative;  
}
.real-bottom{
  background:rgb(246, 161, 34);
}
.close{
  width: 15px;
  height: 15px;
  position:absolute;
  top:10px;
  right:4%;
  padding:10px;
}
.groupList{
  width: 100%;
  margin-top:10px;
}
.real-item{
  width: 100%;
  height: 50px;
  border-bottom:1px solid rgb(245, 245, 245);
}
.real-span,.real-input{
  display:inline-block;
  height: 100%;
  line-height:50px;
  float: left;
  text-align:left;
}
.real-span{
  width: 28%;
  padding-left:3.6%;
  box-sizing:border-box;
}
.real-input{
  width: 72%;
  border:none;
  outline:none;
}
.userList{
  width: 100%;
  padding: 0 0 0 5%;
  box-sizing:border-box;
  overflow:hidden;
}
.family_group{
  width: 100%;
  overflow:auto;
  height: 200px;
  -webkit-overflow-scrolling:touch;
  padding-top:20px;
}
.group-div{
  float: left;
  height: 70px;
  width:18%;
  border-radius:10px;
  margin-right:7%;
  margin-bottom:20px;
  position:relative;
  overflow:hidden;
}
.group-span{
  display:inline-block;
  width: 100%;
  height: 100%;
  position:absolute;
  top:0;
  left:0;
  background:transparent;
  z-index:99;
}
.group-p{
  height: 20px;
  margin-top:50px;
  line-height:20px;
}
.group-user{
  display:inline-block;
  width: 35px;
  height:35px;
  position:absolute;
  top:1px;
  left:50%;
  margin-left:-24.5px;
  padding:4px;
  border-radius:16px;
  border:1px solid rgb(226, 226, 226);
}
.select{
  display:inline-block;
  width: 16px;
  height: 16px;
  position:absolute;
  right:8%;
  bottom:24px;
}

