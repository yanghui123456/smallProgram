/* pages/weihu/7-confrim-msg/7-confrim-msg.wxss */
page,.confrim{
  width:100%;
  height:100%;
  padding:0 0% 45px 0%;
  position: fixed;
  top:0;
  left:0;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  box-sizing: border-box;
}
.confrim-group{
  width:100%;
  min-height: 160px;
  padding:30px 0 30px  90px;
  border-bottom: 1px dashed rgb(207,207,207);
  box-sizing: border-box;
  position: relative;
}
.img-style{
  width:80px;
  height:70px;
  position: absolute;
  top:30px;
  left:8px;
}
.text{
  display: block;
  width:90px;
  position: absolute;
  top:100px;
  left:0;
}
.noVal{
  display: inline-block;
  height:45px;
  line-height: 45px;
  background: rgb(255,250,241);
  color:rgb(255,185,61);
  font-size:13px;
  border:1px dashed rgb(249,195,154);
  border-radius: 25px;
  padding:0 20px;
  margin: 0 10px 10px 0;
  position: relative;
  box-sizing: border-box;
}
.hasVal{
  background: rgb(255,242,219);
  border:none;
  color:rgb(240,105,2);
  position: relative;
}
.disabledVal{
  background: rgb(240, 240, 240);
  color:rgb(200, 200, 200);
  border:none;
}
.phcolor{
  color:rgb(255,185,61);
}
.insertInput{
  width:98%;
  height:100%;
}
.warning{
  position: absolute;
  top:12px;
  right:10px;
  width:20px;
  height:20px;
}
.widthVal{
  width:84%;
}
/*家庭角色弹窗  */
.familyPop,.popMask{
  width:100%;
  height:100%;
  position: fixed;
  top:0;
  left:0;
  z-index:1000;
}
.popMask{
 z-index:999;
}
.pop{
  width: 100%;
  height: 150px;
  background:rgb(245, 245, 245);
  position:fixed;
  bottom:0;
  left:0;
  z-index:1000;
}
.popHead{
  width: 100%;
  height: 30px;
  padding:0 9%;
  box-sizing:border-box;
}
.cancel,.ensure{
  display:inline-block;
  float: left;
  height: 26px;
  padding:5px 22px;
  line-height:18px;
  margin-top:2px;
  background:rgb(189, 192, 197);
  font-size:15px;
  color:white;
  border-radius:5px;
  box-sizing:border-box;
}
.ensure{
  float: right;
  background:rgb(0, 116, 255);
}
.pop_content{
  width: 100%;
  height: 120px;
  background:white;
  padding: 0 5.2%;
  box-sizing:border-box;
}
.contents{
  width: 100%;
  height: 100%;
  border-left:1px dashed rgb(230, 230, 230);
  border-right:1px dashed rgb(230, 230, 230);
  box-sizing:border-box;
   overflow:auto; 
   -webkit-overflow-scrolling: touch; 
}
.content_group{
  width: 100%;
  height: 60px;
  box-sizing:border-box;
}

.groupItem,.selectRelation{
  float: left;
  width: 20%;
  height: 60px;
  box-sizing:border-box;
  position:relative;
}
.groupItem{
  border:1px dashed rgb(230,230,230);
}
.selectRelation{
  background:rgb(255, 250, 240);
  border:1px dashed rgb(2555, 217, 131);
}
.all{
  display:inline-block;
  width: 100%;
  height: 100%;
  background:transparent;
  position:absolute;
  top:0;
  left:0;
}

.groupImg{
  width: 30px;
  height: 27px;
  margin:5px 0 0 25%;
}
.companyImg{
  width:94%;
  margin:5px 0 0 3%;
}
.groupText{
  display: block;
  width: 100%;
  height: 15px;
  font-size:10px;
  color:rgb(173, 173, 173);
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow:hidden;
}
.seeImg,.complate{
  width:50%;
  height:45px;
  line-height: 45px;
  font-size: 13px;
  position: fixed;
  bottom:0;
  z-index:999;
}
.complate{
  left:0;
  border-right:1px solid white;
  box-sizing: border-box;
}
.seeImg{
  right:0;
}
.scrollView{
  width:100%;
  height:100%;
  overflow: scroll;
  position: fixed;
  top:0;
  left:0;
  z-index:2000;
}
.originalImg{
  width:100%;
  height:100%;
  top:0;
  left:0;
}
/*保险期弹层  */
.period_mask{
        width: 100%;
      height: 100%;
      position:absolute;
      top:0;
      left:0;
      z-index:1000;
}
.mask{
          width: 100%;
        height: 100%;
        position:fixed;
        top:0;
        left:0;
        background:black;
        opacity:0.5;
}
.content{
          width: 100%;
          height:135px;
        position:fixed;
        bottom:0;
        left:0;
}
.head{
            width: 100%;
          height: 45px;
          line-height: 45px;
          background:rgb(246, 246, 246);
}
.headCancel{
              display:inline-block;
            height: 100%;
            padding:0 15px 0 30px;
            color:rgb(247, 161, 33);
            position:absolute;
            top:0;
            right:0;
}
.selecte_list{
            width: 100%;
}
.select_item{
              width: 100%;
            height: 45px;
            line-height: 45px;
            border-top:1px solid rgb(245, 245, 245);
            box-sizing:border-box;
}
/*保障责任  */
.dutyGroup{
  width:98%;
  height:45px;
  line-height: 45px;
  position: relative;
  margin-bottom:10px;
}
.guaranDu,.guaranNum{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  height:45px;
  line-height: 45px;
  position: absolute;
  top:0;
}
.guaranDu{
  width:55%;
  padding:0 14px;
  left:0;
  background: rgb(255,242,219);
  color:rgb(240,105,2);
  font-size:13px;
  border-top-left-radius: 25px;
  border-bottom-left-radius: 25px;
  box-sizing: border-box;
}
.whiteLine{
  width:1px;
  height:45px;
  background: white;
  position: absolute;
  top:0;
  left:55%;
  z-index:1000;
}
.unit{
  position: absolute;
  top:0;
  right:40px;
  color:rgb(240,105,2);
  font-size:13px;
  z-index:1000;
}
.width100{
  width:100%;
}
.guaranNum{
  width:45%;
  right:0;
  border-top-right-radius: 25px;
  border-bottom-right-radius: 25px;
  background: rgb(255,250,241);
  color:rgb(255,185,61);
  font-size:13px;
  border:1px dashed rgb(249,195,154);
  padding-left:10px;
  box-sizing: border-box;
}
.gutanhasVal{
  border:none;
  background: rgb(255,242,219);
}
.deletedDuty{
  width:20px;
  height:20px;
  position: absolute;
  top:12px;
  right:20px;
  z-index:1000;
}
.aaa{
  width:100%;
  height:45px;
  position: fixed;
  bottom:0;
  left:0;
  z-index:999;
}
.bbb{
  width:100%;
  height:100%;
  position: fixed;
  top:0;
  left:0;
  padding-bottom:45px;
  box-sizing: border-box;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  z-index:998;
}